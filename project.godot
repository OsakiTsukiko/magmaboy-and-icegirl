config_version=4

_global_script_classes=[ {
"base": "KinematicBody2D",
"class": "Entity2D",
"language": "GDScript",
"path": "res://src/utils/entity/entity.gd"
}, {
"base": "Node",
"class": "Utils",
"language": "GDScript",
"path": "res://src/utils/utils.gd"
} ]
_global_script_class_icons={
"Entity2D": "",
"Utils": ""
}

[application]

config/name="MagmaBoy & IceGirl"
config/description="Multiplayer Two Player Platformer"
run/main_scene="res://src/ui/main_menu/MainMenu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="magmaboy_and_icegirl"
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.png"

[autoload]

Gamestate="*res://src/utils/gamestate.gd"
NetworkLevelManager="*res://src/utils/network_level_manager/NetworkLevelManager.tscn"

[debug]

multirun/number_of_windows=2
multirun/window_distance=0
multirun/add_custom_args=false
multirun/first_window_args=""
multirun/other_window_args=""

[display]

window/size/width=512
window/size/height=300
window/size/test_width=1024
window/size/test_height=600
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/multirun/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

